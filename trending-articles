$(document).ready(function() {

let $loading = $('.loading').hide();
  //removed padding for hot articles

   $(".loading").lettering();
    $loading.show();
$("#trending-articles").parent().removeClass("1 red blue sqs-block-content");
$("#trending-articles").parent().parent().removeClass("2 sqs-block code-block sqs-block-code");
$("#trending-articles").parent().parent().parent().removeClass("3 col sqs-col-12 span-12")
$("#trending-articles").parent().parent().parent().parent().removeClass("4 row sqs-row")



    /*
    //var s = document.createElement("script");
//    s.type = "text/babel";
  //  s.src = "https://8f6f8caa.ngrok.io/trending-articles.injection.js?v=1";
    //$("footer").append(s);
    */
    //---------------articles----------
    class ArticleComponent extends React.Component {
  render() {
    let {title,description,image,url}=this.props

return (<div  className="row sqs-row"><div className="col sqs-col-4 span-4"><div className="sqs-block image-block sqs-block-image" data-aspect-ratio="100" data-block-type="5" id="block-c54079c5ec9286d2faeb"><div className="sqs-block-content"><div className={["sqs-image",(image)?"":"sqs-empty"]}><div className="sqs-image-content" style={{backgroundImage:"url("+image+")",backgroundSize: "cover",paddingBottom: '100.0%'}}></div></div></div></div></div><div className="col sqs-col-8 span-8"><div className="sqs-block html-block sqs-block-html" data-block-type="2" id="block-75a8017ddd7199db03ad"><div className="sqs-block-content">
        <h2><b>{title}</b></h2>
        <p>{description}</p>
      </div>
    </div><div className="sqs-block button-block sqs-block-button" data-block-type="53" id="block-3decbf7d2072b81f7d6b"><div className="sqs-block-content"><div className="sqs-block-button-container--left" data-alignment="left" data-button-size="small">
          <a href="https://politi.cc/{url}" target="_blank" className="sqs-block-button-element--small sqs-block-button-element" data-initialized="true">Read Article</a>
        </div>
      </div>
    </div>
  </div>
</div>)
}
}
ArticleComponent.defaultProps = {url:"",title:"",description:"",image:""}





class App extends React.Component {
constructor(props){super(props);this.state = {data:[]};this.init=this.init.bind(this)}

componentDidMount(){

// alert("hello sicko")
this.init()
}
init(){
axios.get("https://api.politi.cc/v1/news/articles?token=website").then(o=>{
//console.log("o.data.data",o.data.data)
this.setState({  data:o.data.data})
}).catch(e=>console.error("eeee",e))


}
  render() {
    const {data} = this.state

      $loading.hide();
    return (<div>{data.map((o,i)=>{
//moment(o.datePublished).startOf("hour").fromNow()}
    
    return <div key={i}><ArticleComponent title={(o.title)?o.title:o.headline} description={`by ${o.domain}`} image={o.image} url={o.url}/></div>})}</div>);
   
  // })}</div>)
}
}
ReactDOM.render(<App />,document.getElementById('trending-articles'));
})
